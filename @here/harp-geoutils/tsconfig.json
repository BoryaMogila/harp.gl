{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es6",
        "lib": ["es2017", "dom"],
        "module": "commonjs",
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": false,
        "downlevelIteration": true
    }
}
